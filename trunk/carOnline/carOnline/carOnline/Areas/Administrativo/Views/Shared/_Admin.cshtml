<body>
@{
    tblAdministrador administrador = (tblAdministrador)Session["ADMINISTRADOR"];
    tblFuncionario funcionario = (tblFuncionario)Session["FUNCIONARIO"];
    string nomeUusuario = "";

    if (funcionario != null)
    {
        nomeUusuario = funcionario.nome;
    }
    else
    {
        nomeUusuario = administrador.nome;
    }
}
<div id="header-wrapper" class="container">
	<div id="user-account" class="row">
		<div class="threecol">
			<span>Sistema administrativo Car Online</span>
		</div>
		<div class="ninecol last">
			@Html.ActionLink("Sair", "Deslogar", "Login", new { area = "Administrativo" },null)
			<span>|</span>
			<span>Bem-vindo, <strong>@nomeUusuario</strong></span> 
		</div>
	</div> 
	<div id="user-options" class="row">
		<div class="threecol">
			<a href="@Url.Content("~/Administrativo/Principal/")" title="">
				<img class="logo" alt="logo" width="160" style="margin-top:5px;" src="@Url.Content("~/Content/images/Logo.png")">
			</a>
		</div>
		<div class="ninecol last fixed"></div><!-- ninecol last fixed -->
	</div><!-- user-options -->
</div><!-- header-wrapper -->

<div class="container">
	<div class="row">
		<div id="sidebar" class="threecol">
			<ul id="navigation">
                @if (funcionario != null)
                {
                    if (funcionario.idTipoPermissao != 3)
                    {
                        if (funcionario.idTipoPermissao == 1)
                        {                            
                            <li class="first">
					            <a href="#">Administrador<span class="icon-users"></span></a>
					            <ul style="display: none;">
						            <li>@Html.ActionLink("Cadastro Administrador", "Cadastro", "Administrador", new { area = "Administrativo" }, new { @title = "Cadastro de administrador", @class = "Links" })</li>
						            <li>@Html.ActionLink("Consulta Administrador", "Consulta", "Administrador", new { area = "Administrativo" }, new { @title = "Consulta administrador", @class = "Links" })</li>
					            </ul>
				            </li>     
                        }
                        <li class="">
					        <a href="#">Funcionário<span class=""></span></a>
					        <ul style="display: none;">
						        <li>@Html.ActionLink("Cadastro de Funcionário", "Cadastro", "Funcionario", new { area = "Administrativo" }, new { @title = "Cadastro de funcionário", @class = "Links" })</li>
						        <li>@Html.ActionLink("Consulta de Funcionário", "Consulta", "Funcionario", new { area = "Administrativo" }, new { @title = "Consulta de funcionários", @class = "Links" })</li>
					        </ul>
				        </li>  
                    
                        <li class="">
					        <a href="#">Veículos<span class=""></span></a>
					        <ul style="display: none;">
						        <li>@Html.ActionLink("Carros Vendidos", "FormularioRelatorioCarrosVendidos", "Relatorio", new { area = "Administrativo" }, new { @title = "Relatório de Carros Vendidos", @class = "Links" })</li>
						        <li>@Html.ActionLink("Carros Cadastrados", "FormularioRelatorioCarrosCadastrados", "Relatorio", new { area = "Administrativo" }, new { @title = "Relatório de Carros Cadastrados", @class = "Links" })</li>
					        </ul>
				        </li>  
                    }
                }
                else
                {
                    <li class="first">
					    <a href="#">Administrador<span class="icon-users"></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Cadastro Administrador", "Cadastro", "Administrador", new { area = "Administrativo" }, new { @title = "Cadastro de administrador", @class = "Links" })</li>
						    <li>@Html.ActionLink("Consulta Administrador", "Consulta", "Administrador", new { area = "Administrativo" }, new { @title = "Consulta administrador", @class = "Links" })</li>
					    </ul>
				    </li>  
                    <li class="">
					    <a href="#">Funcionário<span class=""></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Cadastro de Funcionário", "Cadastro", "Funcionario", new { area = "Administrativo" }, new { @title = "Cadastro de funcionário", @class = "Links" })</li>
						    <li>@Html.ActionLink("Consulta de Funcionário", "Consulta", "Funcionario", new { area = "Administrativo" }, new { @title = "Consulta de funcionários", @class = "Links" })</li>
					    </ul>
				    </li> 
                    <li class="">
					    <a href="#">Seguradora<span class=""></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Cadastro de Seguradora", "Cadastro", "Seguradora", new { area = "Administrativo" }, new { @title = "Cadastro de Seguradora", @class = "Links" })</li>
						    <li>@Html.ActionLink("Consulta de Seguradora", "Consulta", "Seguradora", new { area = "Administrativo" }, new { @title = "Consulta de Seguradoras", @class = "Links" })</li>
					    </ul>
				    </li> 
                    <li class="">
					    <a href="#">Consulta Usuário<span class=""></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Consulta de Usuário", "Consulta", "Usuario", new { area = "" }, new { @title = "Consulta de Usuários", @class = "Links" })</li>
					    </ul>
				    </li> 
                    <li class="">
					    <a href="#">Veículos<span class=""></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Carros Vendidos", "FormularioRelatorioCarrosVendidos", "Relatorio", new { area = "Administrativo" }, new { @title = "Relatório de Carros Vendidos", @class = "Links" }) </li>
                            <li>@Html.ActionLink("Carros Cadastrados", "FormularioRelatorioCarrosCadastrados", "Relatorio", new { area = "Administrativo" }, new { @title = "Relatório de Carros Cadastrados", @class = "Links" }) </li>
					    </ul>
				    </li> 
                    <li class="">
					    <a href="#">Parâmetros<span class=""></span></a>
					    <ul style="display: none;">
						    <li>@Html.ActionLink("Cadastro de Parâmetros", "Cadastro", "Parametro", new { area = "Administrativo" }, new { @title = "Cadastro de Parâmetros", @class = "Links" }) </li>
                            <li>@Html.ActionLink("Consulta de Parâmetros", "Consulta", "Parametro", new { area = "Administrativo" }, new { @title = "Consulta de Parâmetros", @class = "Links" }) </li>
                            <li>@Html.ActionLink("Parâmetros de Cargo", "Cargo", "Parametro", new { area = "Administrativo" }, new { @title = "Parâmetros de Cargo", @class = "Links" }) </li>
					    </ul>
				    </li>
                    <li class="">
					    <a href="#">Enquete<span class=""></span></a>
					    <ul style="display: none;">
                            <li>@Html.ActionLink("Cadastro de Enquetes", "Cadastro", "Enquete", new { area = "Administrativo" }, new { @title = "Cadastro de Enquetes", @class = "Links" }) </li>
                            <li>@Html.ActionLink("Consulta de Enquetes", "Consulta", "Enquete", new { area = "Administrativo" }, new { @title = "Consulta de Enquetes", @class = "Links" }) </li>
					    </ul>
				    </li>  
                }   
			</ul><!-- NAVIGATION -->
		</div><!-- SIDEBAR -->
        <div id="content" class="ninecol last">
            @RenderBody()
		</div>
		</div><!-- CONTENT -->
	</body>
</html>
